{
    "name": "yogeshgupta/phonepe-laravel",
    "description": "A Laravel package for integrating PhonePe payment gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yogesh Gupta",
            "email": "yk68785@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "YogeshGupta\\PhonePe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YogeshGupta\\PhonePe\\PhonePeServiceProvider"
            ],
            "aliases": {
                "PhonePe": "YogeshGupta\\PhonePe\\Facades\\PhonePe"
            }
        }
    },
    "minimum-stability": "dev"
}
